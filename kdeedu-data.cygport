inherit kf5

NAME="kdeedu-data"
VERSION=15.04.3
RELEASE=1
CATEGORY="KDE"
SUMMARY="Common data for KDE education programs"

ARCH=noarch

OBSOLETES="libkdeedu-common"

src_install() {
	cd ${B}
	kf5_install

	# for compatibility with KDE4 apps
	dodir /usr/share/kde4/apps
	cp -alf ${D}/usr/share/apps/kvtml/ ${D}/usr/share/kde4/apps/
}
